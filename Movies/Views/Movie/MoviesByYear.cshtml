
@{
    ViewBag.Title = "MoviesByYear";
}

<h2>MoviesByYear</h2>
@model IEnumerable<Movies.Models.Movie>
<h2>Movies Released in @ViewBag.Year</h2>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Release Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
<tr>
    <td>@movie.MovieID</td>
    <td>@movie.MovieName</td>
    <td>@movie.DateOfRelease.ToString("yyyy-MM-dd")</td>
</tr>}
    </tbody>
</table>

